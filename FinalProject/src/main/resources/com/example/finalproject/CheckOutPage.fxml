<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #2C7DA0;" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.finalproject.CheckOutController">
   <children>
      <Button layoutX="601.0" layoutY="31.0" mnemonicParsing="false" onAction="#switchToHomePage" text="Back to home page" />
      <TableView fx:id="equipmentTable" layoutX="50.0" layoutY="92.0" prefHeight="408.0" prefWidth="700.0">
        <columns>
          <TableColumn fx:id="makeColumn" prefWidth="75.0" text="Make" />
          <TableColumn fx:id="modelColumn" prefWidth="75.0" text="Model" />
            <TableColumn fx:id="inUseColumn" prefWidth="75.0" text="In Use" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button layoutX="330.0" layoutY="530.0" mnemonicParsing="false" onAction="#checkOut" text="Change use status" />
      <Label layoutX="50.0" layoutY="47.0" />
   </children>
</AnchorPane>
